class Solution {
    public ArrayList<Integer> findDuplicates(int[] A) {
        // code here
        ArrayList<Integer> ans=new ArrayList<>();
        HashSet<Integer> hs=new HashSet<>();
        
        for (int val:A){
            if(hs.contains(val)){
                ans.add(val);
                
            }else{
                hs.add(val);
            }
        }
        Collections.sort(ans);
        return ans;
        
    }
}
